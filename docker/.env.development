# Development Environment Configuration
SERVER_PORT=3001
STORAGE_DIR="/app/server/storage"
UID='1000'
GID='1000'

# Security Settings (Please change these in production)
SIG_KEY='development-sig-key-please-change-in-production'
SIG_SALT='development-sig-salt-please-change-in-production'

# LLM Provider Settings
LLM_PROVIDER='openai'
# Add your OpenAI key here
# OPEN_AI_KEY=your-key-here
# OPEN_MODEL_PREF='gpt-4'

# Vector Database Settings
VECTOR_DB="lancedb"

# Embedding Settings
EMBEDDING_ENGINE="openai"
# Add your OpenAI key here if different from LLM key
# EMBEDDING_MODEL_PREF='text-embedding-ada-002'

# Optional: Authentication
# AUTH_TOKEN="your-auth-token"  # Uncomment and set for authentication
# JWT_SECRET="your-jwt-secret"  # Required if AUTH_TOKEN is set

# System Settings
DISABLE_TELEMETRY=true 